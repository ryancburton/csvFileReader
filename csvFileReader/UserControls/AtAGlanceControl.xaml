<UserControl x:Class="csvFileReader.UserControls.AtAGlanceControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:csvFileReader.UserControls"
             mc:Ignorable="d" Height="188.151" Width="234.96">
    <Grid Background="White">
        <Label x:Name="numberOfContacts" Content="{Binding ContactsImported}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="68" Width="93" FontSize="48" Foreground="red"/>
        <Label x:Name="numberOfCompanies" Content="{Binding CompaniesImported}" HorizontalAlignment="Left" Margin="0,68,0,0" VerticalAlignment="Top" Height="71" Width="93" FontSize="48" Foreground="orange"/>
        <Label x:Name="contacts" Content="Contacts" HorizontalAlignment="Left" Margin="102,30,0,0" VerticalAlignment="Top" Width="84" Height="38"/>
        <Label x:Name="companies" Content="Companies" HorizontalAlignment="Left" Margin="96,95,0,0" VerticalAlignment="Top" Width="90" Height="44"/>
        <Label x:Name="atAGlance" Content="At a glance" HorizontalAlignment="Left" Margin="55,144,0,0" VerticalAlignment="Top" Width="131"/>
    </Grid>
</UserControl>
